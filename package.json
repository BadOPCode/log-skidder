{
  "name": "log-skidder",
  "version": "1.0.0",
  "description": "Provides a common logging interface.",
  "main": "lib/index.js",
  "scripts": {
    "build": "parcel build src/index.ts -d lib",
    "build:w": "parcel src/tester.html",
    "clean": "rm -r lib coverage .nyc_output dist",
    "lint": "tslint",
    "test": "alsatian \"__tests__/**/*.test.ts\" --tap | tap-bark",
    "test:coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report"
  },
  "keywords": [
    "console",
    "log"
  ],
  "author": "Shawn Rapp",
  "license": "MIT",
  "devDependencies": {
    "alsatian": "^3.2.1",
    "nyc": "^15.0.0",
    "parcel-bundler": "^1.12.4",
    "tap-bark": "^2.3.1",
    "ts-nodash": "^1.1.6",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {}
}
